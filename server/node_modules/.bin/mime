#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/silentfellow/Projects/sih/server/node_modules/mime/node_modules:/home/silentfellow/Projects/sih/server/node_modules:/home/silentfellow/Projects/sih/node_modules:/home/silentfellow/Projects/node_modules:/home/silentfellow/node_modules:/home/node_modules:/node_modules:/home/silentfellow/Projects/sih/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/silentfellow/Projects/sih/server/node_modules/mime/node_modules:/home/silentfellow/Projects/sih/server/node_modules:/home/silentfellow/Projects/sih/node_modules:/home/silentfellow/Projects/node_modules:/home/silentfellow/node_modules:/home/node_modules:/node_modules:/home/silentfellow/Projects/sih/server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mime/cli.js" "$@"
else
  exec node  "$basedir/../mime/cli.js" "$@"
fi
